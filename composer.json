{
    "name": "mtholyoke/jorge",
    "description": "MHCâ€™s command-line tool for automating development tasks",
    "keywords": [ "cli", "mtholyoke" ],
    "homepage": "https://www.mtholyoke.edu",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/composer": "^1.10.22",
        "php": ">=7.3.0",
        "psr/log": "^1.1.4",
        "symfony/console": "^5.2",
        "symfony/yaml": "^5.2",
        "symfony/process": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "MountHolyoke\\Jorge\\": "src/",
            "MountHolyoke\\JorgeTests\\": "tests/"
        }
    },
    "bin": [
        "bin/jorge"
    ],
    "scripts": {
        "phpunit": [
            "@putenv XDEBUG_MODE=coverage",
